ole.log(error.response.data.error);\n    }\n  },\n  methods: {\n    paginate(currentPage) {\n      const start = (currentPage - 1) * this.perPage;\n      this.reviewList2 = this.reviewList.slice(start, start + this.perPage);\n    }\n  },\n  computed: {}\n};\n</script>\n\n<style lang=\"scss\" scoped></style>\n"]}]}