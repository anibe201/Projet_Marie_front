f (this.searchTxt != \"\") {\n        const text = this.searchTxt;\n        newRoute += \"?q=\" + text;\n        this.$router.push({ path: newRoute });\n      }\n    }\n  }\n};\n</script>\n\n<style scoped lang=\"scss\"></style>\n"]}]}